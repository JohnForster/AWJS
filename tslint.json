{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "src/zOldGameEngine"
        ]
    },
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": [true, "single", "avoid-escape"],
        "object-literal-sort-keys": false,
        "array-type": [true, "array"],
        "no-empty": false
    },
    "rulesDirectory": []
}